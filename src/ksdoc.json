{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|jsdocs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./jsdocs/",
    "template": "node_modules/better-docs",
    "readme": "./README.md"
  }
}
